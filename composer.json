{
    "name": "netsells/eloquent-filters",
    "description": "A simple package to help you easily organise model query logic",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "eloquent",
        "models",
        "filter",
        "search"
    ],
    "authors": [
        {
            "name": "Adam Hutchison",
            "email": "adam.hutchison@netsells.co.uk"
        }
    ],
    "require": {
        "php": "^8.0",
        "netsells/attribute-finder": "dev-task/AAD-8"
    },
    "require-dev": {
        "orchestra/testbench": "^6.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Netsells\\EloquentFilters\\EloquentFiltersServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Netsells\\EloquentFilters\\": "src",
            "Netsells\\EloquentFilters\\Tests\\": "tests"
